{
  "name": "token_back",
  "version": "1.0.0",
  "description": "token back",
  "main": "truffle-config.js",
  "dependencies": {
    "@openzeppelin/contracts": "^3.2.0",
    "@openzeppelin/test-helpers": "^0.5.6",
    "@truffle/hdwallet-provider": "^1.1.0",
    "@uniswap/lib": "1.1.1",
    "@uniswap/v2-core": "1.0.0",
    "dotenv": "^8.2.0",
    "solc": "0.6.6",
    "truffle": "5.1.46",
    "truffle-contract": "^4.0.31",
    "tslib": "^1.13.0"
  },
  "scripts": {
    "dep:all": "yarn dep:migrate && yarn dep:allSetup",
    "dep:migrate": "npx truffle migrate --reset",
    "dep:allSetup": "npx truffle exec scripts/dep/end2end.js",
    "dep:transferOwner": "npx truffle exec scripts/dep/transferIPhoneOwnership.js",
    "dep:mintSale": "npx truffle exec scripts/dep/mintPhoneTokenForSale.js",
    "dep:addTokens": "npx truffle exec scripts/dep/addTokensToSale.js",
    "dep:approveStore": "npx truffle exec scripts/dep/approveStore.js",
    "dep:addPhonePool": "npx truffle exec scripts/dep/addPhoneTokenPool.js",
    "dep:addOtherPool": "npx truffle exec scripts/dep/addOtherTokensPool.js",
    "dep:createDevices": "npx truffle exec scripts/dep/createDevices.js",
    "dep:getSpecById": "npx truffle exec scripts/dep/getSpecById.js",
    "dep:getSpecByModel": "npx truffle exec scripts/dep/getSpecByModel.js",
    "dep:updateSpec": "npx truffle exec scripts/dep/updateSpec.js",
    "nonDep:allSetup": "npx truffle exec scripts/nonDep/end2end.js",
    "nonDep:transferOwner": "npx truffle exec scripts/nonDep/transferIPhoneOwnership.js",
    "nonDep:mintSale": "npx truffle exec scripts/nonDep/mintPhoneTokenForSale.js",
    "nonDep:addTokens": "npx truffle exec scripts/nonDep/addTokensToSale.js",
    "nonDep:approveStore": "npx truffle exec scripts/nonDep/approveStore.js",
    "nonDep:addPhonePool": "npx truffle exec scripts/nonDep/addPhoneTokenPool.js",
    "nonDep:addOtherPool": "npx truffle exec scripts/nonDep/addOtherTokensPool.js",
    "nonDep:createDevices": "npx truffle exec scripts/nonDep/createDevices.js",
    "nonDep:getSpecById": "npx truffle exec scripts/nonDep/getSpecById.js",
    "nonDep:getSpecByModel": "npx truffle exec scripts/nonDep/getSpecByModel.js",
    "nonDep:updateSpec": "npx truffle exec scripts/nonDep/updateSpec.js",
    "test": "npx truffle test",
    "test-factory": "npx truffle test test/Factory.test.js",
    "test-sale": "npx truffle test test/Sale.test.js"
  }
}
